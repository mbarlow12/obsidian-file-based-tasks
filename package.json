  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.33",
    "@types/redux-orm": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "obsidian": "^0.15.1",
    "rollup": "^2.32.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
