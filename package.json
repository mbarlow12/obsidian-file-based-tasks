{
  "name": "obsidian-sample-plugin",
  "version": "0.12.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:wp": "webpack --progress --color --mode=development",
    "build:wpp": "webpack --progress --color --mode=production",
    "test": "node esbuild.config.mjs test",
    "test:prod": "node esbuild.config.mjs production test"
  },
  "keywords": [],
