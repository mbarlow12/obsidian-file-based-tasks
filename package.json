{
  "name": "tasqs",
  "version": "0.12.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:wp": "webpack --progress --color --mode=development",
    "build:wpp": "webpack --progress --color --mode=production",
    "lint": "eslint ./src --fix && eslint ./test --fix && tsc --noEmit --pretty",
    "test": "node esbuild.config.mjs test",
    "test:prod": "node esbuild.config.mjs production test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.33",
    "@types/redux-orm": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "esbuild": "0.13.12",
    "eslint": "^8.2.0",
    "jest": "^29.0.0",
    "obsidian": "latest",
    "prettier": "^3.1.1",
    "ts-jest": "^29.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "chrono-node": "^2.3.8",
    "path-browserify": "^1.0.1",
    "rrule": "^2.6.9"
  }
}
